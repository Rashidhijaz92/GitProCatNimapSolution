        <td>
            @Html.DisplayFor(modelItem => item.ProdctName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_ProductId }) |
